{
  "name": "cryptoview",
  "version": "1.0.0",
  "description": "An app that shows multiple crypto currency exchange prices",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "Electron",
    "Cryptocurrency"
  ],
  "build": {
    "appId": "com.tahnik.cryptoview",
    "win": {
      "target": [
        "portable"
      ]
    }
  },
  "author": "Tahnik Mustasin",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.49.0"
  },
  "optionalDependencies": {
    "7zip-bin-mac": "^1.0.1",
    "7zip-bin-linux": "^1.1.0",
    "7zip-bin-win": "^2.1.1"
  }
}
